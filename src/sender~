#!/usr/bin/env python   
import sys
 
def send():
	f = open('communication', 'w')
	sent = 1
	obstacle1 = 2
	obstacle2 = 3
	obstacle3 = 4
	f.write(str(sent))
	f.write(' ')
	f.write(str(obstacle1))
	f.write(' ')
	f.write(str(obstacle2))
	f.write(' ')
	f.write(str(obstacle3))
	f.close
      
if __name__ == '__main__':
	send()
